<UserControl x:Class="Basilisk.Controls.WeekScheduleVisualizer"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Basilisk.Controls"
             xmlns:chart="clr-namespace:System.Windows.Forms.DataVisualization.Charting;assembly=System.Windows.Forms.DataVisualization"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <WindowsFormsHost>
            <chart:Chart x:Name="chart">
                <chart:Chart.ChartAreas>
                    <chart:ChartArea />
                </chart:Chart.ChartAreas>
            </chart:Chart>
        </WindowsFormsHost>
    </Grid>
</UserControl>
